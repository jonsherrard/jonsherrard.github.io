@css{
  @font-face {
      font-family: 'geomancyhairline';
      src: url('../fonts/geomancy_-_hairline-webfont.eot');
      src: url('../fonts/geomancy_-_hairline-webfont.eot?#iefix') format('embedded-opentype'),
           url('../fonts/geomancy_-_hairline-webfont.woff') format('woff'),
           url('../fonts/geomancy_-_hairline-webfont.ttf') format('truetype'),
           url('../fonts/geomancy_-_hairline-webfont.svg#geomancyhairline') format('svg');
      font-weight: normal;
      font-style: normal;

  }




  @font-face {
      font-family: 'geomancyextra_bold';
      src: url('../fonts/geomancy_-_extra_bold-webfont.eot');
      src: url('../fonts/geomancy_-_extra_bold-webfont.eot?#iefix') format('embedded-opentype'),
           url('../fonts/geomancy_-_extra_bold-webfont.woff') format('woff'),
           url('../fonts/geomancy_-_extra_bold-webfont.ttf') format('truetype'),
           url('../fonts/geomancy_-_extra_bold-webfont.svg#geomancyextra_bold') format('svg');
      font-weight: normal;
      font-style: normal;

  }
}
